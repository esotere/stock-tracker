{
  "name": "stock-tracker",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "8.9.1",
    "yarn": "1.7.0",
    "npm": "6.1.0"
  },
  "dependencies": {
    "alphavantage": "^1.1.1",
    "axios": "^0.18.0",
    "bson": "^3.0.0",
    "concurrently": "^3.6.0",
    "express": "^4.16.3",
    "mongoose": "^5.2.3",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "start": "react-scripts start if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "build": "react-scripts build && cd client && npm run build",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon server\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && yarn install",
    "heroku-postbuild": "npm run build"
  }
}
